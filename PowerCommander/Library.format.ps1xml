<?xml version="1.0" encoding="utf-8" ?>
<Configuration>
  <ViewDefinitions>
    <View>
      <Name>KeeperSecurity.Authentication.AuthCommon_TableView</Name>
      <ViewSelectedBy>
        <TypeName>KeeperSecurity.Authentication.AuthCommon</TypeName>
      </ViewSelectedBy>
      <TableControl>
        <TableHeaders>
          <TableColumnHeader>
            <Label>Username</Label>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Enterprise Admin</Label>
          </TableColumnHeader>
          <TableColumnHeader>
            <Label>Keeper Endpoint</Label>
          </TableColumnHeader>
        </TableHeaders>
        <TableRowEntries>
          <TableRowEntry>
            <Wrap/>
            <TableColumnItems>
              <TableColumnItem>
                <PropertyName>Username</PropertyName>
              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>$_.AuthContext.IsEnterpriseAdmin</ScriptBlock>
              </TableColumnItem>
              <TableColumnItem>
                <ScriptBlock>$_.Endpoint.Server</ScriptBlock>
              </TableColumnItem>
            </TableColumnItems>
          </TableRowEntry>
        </TableRowEntries>
      </TableControl>
    </View>
    
    <View>
      <Name>KeeperSecurity.Authentication.AuthCommon_ListView</Name>
      <ViewSelectedBy>
        <TypeName>KeeperSecurity.Authentication.AuthCommon</TypeName>
      </ViewSelectedBy>
      <ListControl>
        <ListEntries>
          <ListEntry>
            <ListItems>
              <ListItem>
                <PropertyName>Username</PropertyName>
              </ListItem>
              <ListItem>
                <Label>Enterprise Admin</Label>
                <ScriptBlock>$_.AuthContext.IsEnterpriseAdmin</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>Keeper Endpoint</Label>
                <ScriptBlock>$_.Endpoint.Server</ScriptBlock>
              </ListItem>
              <ListItem>
                <Label>Configuration</Label>
                <PropertyName>ConfigurationInfo</PropertyName>
              </ListItem>
            </ListItems>
          </ListEntry>
        </ListEntries>
      </ListControl>
    </View>

