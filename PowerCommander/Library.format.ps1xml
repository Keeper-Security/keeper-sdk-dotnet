            <ViewSelectedBy>
                <TypeName>KeeperSecurity.Authentication.AuthCommon</TypeName>
            </ViewSelectedBy>
            <TableControl>
                <TableHeaders>
                    <TableColumnHeader>
                        <Label>Username</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Enterprise Admin</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>Keeper Endpoint</Label>
